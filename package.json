{
  "name": "ts-electron-tools",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron dist/main.js",
    "prebuild": "tsc --project build/tsconfig-webpack.json",
    "build": "webpack --config build/.out/webpack.config.js --mode production",
    "predev": "tsc --project build/tsconfig-webpack.json",
    "dev": "webpack serve --config build/.out/webpack.config.js --mode development --open",
    "electron": "npm run build && electron dist/main.js",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron dist/main.js\"",
    "dev-run": "cross-env NODE_ENV=development tsc --project build/tsconfig-webpack.json && cross-env NODE_ENV=development node build/.out/dev.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@bpmn-io/properties-panel": "^3.13.0",
    "@types/electron": "^1.6.12",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.21",
    "bpmn-js": "^15.1.3",
    "bpmn-js-properties-panel": "^5.21.0",
    "bpmn-js-token-simulation": "^0.31.1",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "electron": "^22.3.27",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "zustand": "4.5.6"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "i18next": "^25.6.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^7.9.5"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "MyApp",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
